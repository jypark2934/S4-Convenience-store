{
  "_version": "1.17.0",
  "sap.app": {
    "id": "project5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.7.1",
      "toolsId": "61e94452-8065-43d3-b0d1-b2c35296cf10"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZZ2C_GOODPACK_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "itemHeaderService": {
        "uri": "/sap/opu/odata/sap/ZZ2C_STOREM_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [],          
          "odataVersion": "2.0"
        }
    }
  }
},

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.71.24",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project5.i18n.i18n"
        }
      },
      "main": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "main2": {
        "dataSource": "itemHeaderService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project5.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name" : "SecondView",
          "pattern" : "second",
          "target" : "SecondTarget"
        },
        {
          "name" : "ThirdView",
          "pattern" : "third",
          "target" : "ThirdTarget"
        },
        {
          "name" : "FourthView",
          "pattern" : "fourth",
          "target" : "FourthTarget"
        }
      ],
      "targets": {
        "TargetView1": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View1",
          "viewName": "View1"
        },
        "SecondTarget" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View2",
          "viewName": "View2"
        },
        "ThirdTarget" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View3",
          "viewName": "View3"
        },
        "FourthTarget" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View4",
          "viewName": "View4"
        }
      }
    },
    "rootView": {
      "viewName": "project5.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}